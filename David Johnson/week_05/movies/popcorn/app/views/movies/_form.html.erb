<%= form_for @movie do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :released %>
  <%= f.text_field :released %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :cast %>
  <%= f.text_field :cast %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.submit %>
<% end %>
